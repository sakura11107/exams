<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTO Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sxt.dao.PClassDao">

    <select id="getAllClass" resultType="com.sxt.pojo.PClass">
        select * from pclass
    </select>
    <select id="GetClassId" resultType="com.sxt.pojo.PClass">
        select * from pclass where classid=#{classid}
    </select>
</mapper>