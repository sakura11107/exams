<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTO Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sxt.dao.CourseDao">
    <select id="getAllById" resultType="com.sxt.pojo.Course">
        select * from course where cno=#{cno}
    </select>
    <select id="getAllCourse" resultType="com.sxt.pojo.Course">
        select * from course
    </select>
</mapper>